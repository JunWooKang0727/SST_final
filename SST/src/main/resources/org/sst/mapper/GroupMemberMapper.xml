<mapper namespace="org.sst.mapper.GroupMemberMapper"> <!-- 동일한 이름의 인터페이스를 만들어야한다 -->
	<cache />

	<select id="getGm_numById" resultType="org.sst.domain.GroupMemberVO">
		select * from groupmember where m_id = #{m_id} and g_num = #{g_num}
	</select>
	
	
	
</mapper>















